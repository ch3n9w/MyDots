##export FZF_DEFAULT_OPTS="--height 50% --layout=reverse --border --preview '(highlight -O ansi {} || cat {}) 2> /dev/null | head -500'"
#export FZF_DEFAULT_OPTS="--height 80% --layout=reverse --border --preview 'bat --color=always --italic-text=always --style=numbers,changes,header --line-range :500 {} '"
export FZF_DEFAULT_OPTS="--height 80% --layout=reverse --border --preview '~/Documents/Other/fzf-scope.sh {} '"
export FZF_DEFAULT_COMMAND="fd --exclude={.wine,.git,.idea,.vscode,.sass-cache,node_modules,build,.local,.steam,.m2,.rangerdir,.ssh,.ghidra,.mozilla} --type f --hidden --follow"

vim_fzf() {
    local fzf_result=$(fzf)
    echo $fzf_result
    if [ -z "$fzf_result" ]
    then
        echo "No file specific, Interupt tne vim command"
    else
        nvim  $fzf_result
    fi
}

xdg_fzf() {
    cd ~/Documents/sec_public_articles/
    local fzf_result=$(fzf)
    if [ -z "$fzf_result" ]
    then
        echo "No file specific, Interupt tne xdg-open command"
    else
        xdg-open "$fzf_result"
    fi
}


